{
  "name": "solar-commerce-backend",
  "version": "1.0.0",
  "description": "Complete e-commerce platform for solar products with advanced analytics",
  "author": "Solar Commerce Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --watch",
    "test:e2e:cov": "jest --config ./test/jest-e2e.json --coverage",
    "analytics:generate": "node dist/scripts/generate-analytics.js",
    "analytics:health": "node dist/scripts/analytics-health-check.js",
    "docs:generate": "npm run build && node dist/main.js --export-docs",
    "docs:openapi": "npm run build && node -e \"require('./dist/main.js').setupSwagger && console.log('OpenAPI exported to docs/api/') || console.log('Run: npm run docs:generate')\"",
    "docs:serve": "npm run start:dev",
    "db:migrate": "ts-node scripts/migrate.ts",
    "db:seed": "ts-node scripts/seed.ts",
    "db:migrate-isadmin": "ts-node scripts/migrate-remove-isadmin.ts",
    "db:migrate-wholesale-to-merchant": "ts-node scripts/migrate-wholesale-to-merchant.ts",
    "i18n:import": "node scripts/import-i18n-translations.js",
    "i18n:test": "node scripts/test-i18n-api.js",
    "migrate:capabilities": "ts-node scripts/run-migration.ts",
    "migrate:cleanup": "ts-node scripts/cleanup-capabilities.ts",
    "db:backfill-product-timestamps": "ts-node scripts/backfill-product-timestamps.ts",
    "migration:full": "npm run migrate:capabilities && echo 'Migration completed. Run npm run migrate:cleanup to remove old data.'",
    "update:super-admin": "ts-node scripts/update-super-admin-permissions.ts",
    "fix-nan-values": "ts-node scripts/fix-nan-stock-values.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/mongoose": "^11.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@simplewebauthn/server": "^13.2.2",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "date-fns": "^2.30.0",
    "express-rate-limit": "^6.8.0",
    "firebase-admin": "^13.5.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.0",
    "js-yaml": "^4.1.0",
    "mongoose": "^7.0.0",
    "nodemailer": "^7.0.9",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "puppeteer": "^24.25.0",
    "rate-limiter-flexible": "^8.1.0",
    "redis": "^4.6.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.4",
    "swagger-ui-express": "^5.0.0",
    "twilio": "^5.10.3",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^7.0.2",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
